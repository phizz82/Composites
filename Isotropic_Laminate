clear all 
clc

%% Known Values 

E1 = [181,204,138,38.6,76];
E2 = [10.3,18.5,8.96,8.27,5.5]; 
v12 = [0.28,0.23,0.30,0.26,0.34];
v21 = (v12.*E2)./E1;
G12 = [7.17,5.59,7.1,4.14,2.3];

%% Reduced Stiffness Calculations

Q11 = E1./(1-(v12.*v21));
Q12 = (v12.*E2)./(1-v12.*v21);
Q22 = E2./(1-(v12.*v21));
Q66 = G12; 

%% Determination by isotropic equations

v = (Q11+Q22+(6.*Q12)-(4.*Q66))./((3.*Q11)+(3.*Q22)+(2.*Q12)+(4.*Q66));
G = (Q11+Q22-(2.*Q12)+(4.*Q66))/8; 
E = (2*(1+v)).*G;
